{
    "api_path": {
        "description": "API接口路径",
        "type": "string",
        "default": "/api/render/image"
    },
    "auth_token": {
        "description": "认证令牌",
        "type": "string",
        "default": ""
    },
    "server_host": {
        "description": "服务监听地址",
        "type": "string",
        "default": "0.0.0.0"
    },
    "server_port": {
        "description": "服务端口",
        "type": "int",
        "default": 8080
    },
    "template_notification_enabled": {
        "description": "启用通知模板",
        "type": "bool",
        "default": true
    },
    "template_notification_width": {
        "description": "通知模板渲染宽度",
        "type": "int",
        "default": 800
    },
    "template_notification_quality": {
        "description": "通知模板图片质量",
        "type": "string",
        "default": "high"
    }
}